--- a/mm/damon/core.c
+++ b/mm/damon/core.c
@@ -2175,7 +2175,7 @@ static void damos_set_effective_quota(struct damos_quota *quota)
 static void damos_trace_esz(struct damon_ctx *c, struct damos *s,
