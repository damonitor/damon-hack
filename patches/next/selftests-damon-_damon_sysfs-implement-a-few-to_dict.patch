From: SeongJae Park <sj@kernel.org>
Date: Sun, 13 Jul 2025 13:03:12 -0700
Subject: [PATCH] selftests/damon/_damon_sysfs: implement a few to_dict()

Signed-off-by: SeongJae Park <sj@kernel.org>
---
 tools/testing/selftests/damon/_damon_sysfs.py | 13 +++++++++++++
 1 file changed, 13 insertions(+)

diff --git a/tools/testing/selftests/damon/_damon_sysfs.py b/tools/testing/selftests/damon/_damon_sysfs.py
index f587e117472e..7d2427913e19 100644
--- a/tools/testing/selftests/damon/_damon_sysfs.py
+++ b/tools/testing/selftests/damon/_damon_sysfs.py
@@ -376,6 +376,9 @@ class DamonCtx:
                 return err
         return None
 
+    def to_dict(self):
+        return {}
+
 class Kdamond:
     state = None
     pid = None
@@ -524,6 +527,11 @@ class Kdamond:
         return write_file(os.path.join(self.sysfs_dir(), 'state'),
                          'commit_schemes_quota_goals')
 
+    def to_dict(self):
+        return {
+                'contexts': [ctx.to_dict() for ctx in self.contexts],
+                }
+
 class Kdamonds:
     kdamonds = []
 
@@ -553,3 +561,8 @@ class Kdamonds:
             if err is not None:
                 return err
         return None
+
+    def to_dict(self):
+        return {
+                'kdamonds': [kd.to_dict() for kd in self.kdamonds],
+                }
-- 
